<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Daily Build">
  <listeners>
    <listener class-name="stest.stabila.wallet.common.client.utils.DailyBuildReport"></listener>
  </listeners>
  <test name="stest">
    <packages>
      <package name="stest.stabila.wallet.dailybuild.*">
        <exclude name="stest.stabila.wallet.dailybuild.multisign"></exclude>
        <exclude name="stest.stabila.wallet.dailybuild.operationupdate"></exclude>
      </package>
    </packages>

  </test>
</suite>